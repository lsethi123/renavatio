<% cols = Visit.columns_hash.keys.map{|p| p.to_s.camelcase } %>
<% display_columns = Visit.columns_hash.keys.map{|p| p } %>
<% cols = cols.map{|c| c.split.join(" ")} %>
<% visit = Visit.new() %>

<%= render :partial => "shared/index",
  :locals => {
    :heading => cols,
    :object => @visits,
    :columns => display_columns,
    :title => "Visits",
    :model_object => visit,
    :new_path => new_patient_visit_path
  }
%>